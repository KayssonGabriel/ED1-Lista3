package application;

import java.util.Scanner;

import util.Calc;

public class Algoritimo463 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Calc calc = new Calc();

		System.out.print("digite angulo em graus: ");
		calc.setAng(sc.nextDouble());

		sc.close();

	}

}
