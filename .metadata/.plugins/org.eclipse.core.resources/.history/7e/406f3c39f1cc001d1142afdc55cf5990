package algoritmo490;

public class Calc {
	int restantes(String[] vet, int tam, String x) {
        int cont = 0;

        for (int i = 0; i < tam; i++) {
            if (vet[i].equals(x)) {
                vet[i] = "1";
                cont++;
            }
        }

        return cont;
    }
}


}
